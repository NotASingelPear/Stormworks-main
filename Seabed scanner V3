		-- 		y = f( sin_i*j +.5)
				
		-- 		if not toDraw[x] then
		-- 			toDraw[x] = {}
		-- 		end
				
		-- 		if not toDraw[x][y] then
		-- 			dis = j*(set_range/35)
		-- 			req_x = Round10( pos.x + cos(-azimuth + i) * dis)
		-- 			req_y = Round10( pos.y + sin(-azimuth + i) * dis)
					
