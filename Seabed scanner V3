	gridFilter = 10
	heightFilter = 1
	rec_size = 2
	
	pos = {
	    	x = gN(1),
	    	y = 0,--gN(3),		--switched in physics sensor
	    	z = 0--gN(2)
	    }
    -- Looking towards laser sensor from behind
